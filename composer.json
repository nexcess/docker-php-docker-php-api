{
    "name": "docker-php/docker-php-api",
    "license": "MIT",
    "type": "library",
    "description": "Docker API generated files from OpenAPI Specification",
    "autoload": {
        "psr-4": {
            "Docker\\API\\": "src/"
        }
    },
    "require": {
        "php": ">=7.1",
        "jane-php/open-api-runtime": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "friendsofphp/php-cs-fixer": "^2.9",
        "jane-php/open-api-3": "^7.0",
        "jane-php/json-schema": "^7.0",
        "amphp/artax": "^3.0"
    },
    "suggest": {
        "amphp/artax": "To use the async api"
    },
    "extra": {
        "branch-alias": {
            "4.1.36": "4.1.36.x-dev"
        }
    },
    "prefer-stable": true
}
